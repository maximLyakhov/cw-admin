{
  "name": "cw-booking",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "force-install": "npm i --force",
    "start": "npm run start:a & npm run start:b",
    "start:a": "ng serve -o --watch --port=5001 -c admin-development",
    "start:b": "ng serve -o --watch --port=5000 -c booking-development",
    "local": "npm run local:a & npm run local:b",
    "local-backend": "cd ../cap_plat && nodemon --watch './**/*.go' --signal SIGTERM --exec 'go run' main.go",
    "local:a": "ng serve -o --watch --port=5001 -c admin-local",
    "local:b": "ng serve -o --watch --port=5000 -c booking-local",
    "build": "npm run build:a & npm run build:b",
    "build:a": "ng build -c admin-production",
    "build:b": "ng build -c booking-production",
    "prewba": "ng build -c wba --stats-json",
    "wba": "webpack-bundle-analyzer build/cw-admin/stats.json"
  },
  "dependencies": {
    "@angular/animations": "~13.3.4",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "~13.3.4",
    "@angular/compiler": "~13.3.4",
    "@angular/core": "~13.3.4",
    "@angular/forms": "~13.3.4",
    "@angular/localize": "^13.3.4",
    "@angular/material": "^13.3.4",
    "@angular/platform-browser": "~13.3.4",
    "@angular/platform-browser-dynamic": "~13.3.4",
    "@angular/router": "~13.3.4",
    "@fontsource/inter": "^4.5.7",
    "@ng-select/ng-select": "^8.1.1",
    "@ngneat/until-destroy": "^9.0.1",
    "@ngxs-labs/dispatch-decorator": "^4.0.2",
    "@ngxs-labs/select-snapshot": "^4.0.0",
    "@ngxs/router-plugin": "^3.7.3",
    "@ngxs/storage-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "@ngxs/websocket-plugin": "^3.7.3",
    "@stripe/stripe-js": "^1.23.0",
    "dayjs": "^1.11.1",
    "luxon": "^2.3.0",
    "ng2-completer": "^9.0.1",
    "ng2-smart-table": "^1.7.2",
    "ngx-daterangepicker-material": "^5.0.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.4",
    "@ngxs/devtools-plugin": "^3.7.3",
    "@types/luxon": "^2.0.9",
    "@types/node": "^12.11.1",
    "typescript": "^4.5.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
